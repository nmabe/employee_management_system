<% if @employee.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
        The Form Contains <%= pluralize(@employee.errors.count, "error") %>
    </div>
    <ul>
        <% @employee.errors.full_messages.each do | message | %>
            <li><%= message %></li>
        <% end %>
    </ul>
  </div>
<% end %>